<h2>Enrolled Students</h2>
<% @course.students.each do |student| %>
    <p>
      <strong>First Name:</strong>
      <%= student.first_name %>
    </p>
    <p>
      <strong>Last Name:</strong>
      <%= student.last_name %>
    </p>
    <p>
      <strong>Email:</strong>
      <%= student.email %>
    </p>
    <p><%= link_to 'Delete student', [student.course, student],
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
    </p>
<% end %>